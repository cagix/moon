[
 [cdq.start/when* [gdl.start/operating-system
                   :os/mac-osx
                   [[clojure.lwjgl.system.configuration/set-glfw-library-name! "glfw_async"]
                    [clojure.java.awt.taskbar/set-icon! "moon.png"]]]]
 [gdl.start/start! {:config {:title "Cyber Dungeon Quest"
                             :windowed-mode {:width 1440
                                             :height 900}
                             :foreground-fps 60}
                    :listener {:create  [cdq.application/create! "config.edn"]
                               :dispose cdq.application/dispose!
                               :render  cdq.application/render!
                               :resize  cdq.application/resize!}}]
 ]
